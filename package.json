{
  "name": "@smsoft/sui-core",
  "version": "1.0.4",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "styles",
    "lib"
  ],
  "author": "el1191 <el1191@woyd.ru>",
  "repository": "git@github.com:mbuyakov/SUI.CORE.git",
  "bugs": {
    "url": "https://github.com/mbuyakov/SUI.CORE/issues"
  },
  "homepage": "https://github.com/mbuyakov/SUI.CORE#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf lib",
    "build": "tsc --module commonjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "precommit": "lint-staged",
    "unpublish": "node ./unpublish.js"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "lint-staged": "^8.0.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "antd": "^3.15.0",
    "autobind-decorator": "^2.4.0"
  }
}
