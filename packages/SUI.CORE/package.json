{
  "name": "@smsoft/sui-core",
  "version": "2.6.0",
  "main": "dist/index.js",
  "module": "dist/sui-core.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "lint": "tslint -t verbose -p tsconfig.json -c ../../tslint.json 'src/**/*.ts?(x)'"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testMatch": [
      "**/*.(spec|test).{ts,tsx}"
    ]
  },
  "prettier": {
    "printWidth": 8000,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.9",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "husky": "^3.0.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/history": "^4.7.2",
    "antd": "^3.21.2",
    "apollo-client": "^2.6.3",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
