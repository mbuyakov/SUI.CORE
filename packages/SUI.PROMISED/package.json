{
  "name": "@smsoft/sui-promised",
  "version": "5.7.3",
  "main": "dist/index.js",
  "module": "dist/sui-promised.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tslint -t verbose -p tsconfig.json -c ./tslint.json 'src/**/*.ts?(x)'",
    "ci": "yarn lint && yarn build"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testMatch": [
      "**/*.(spec|test).{ts,tsx}"
    ]
  },
  "prettier": {
    "printWidth": 8000,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "tsdx": "^0.9.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@smsoft/sui-core": "^5.7.3"
  }
}
