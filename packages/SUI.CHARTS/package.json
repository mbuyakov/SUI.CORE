{
  "name": "@smsoft/sui-charts",
  "version": "5.4.0",
  "main": "dist/index.js",
  "module": "dist/sui-charts.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tslint -t verbose -p tsconfig.json -c ./tslint.json 'src/**/*.ts?(x)'",
    "ci": "yarn lint && yarn build"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testMatch": [
      "**/*.(spec|test).{ts,tsx}"
    ]
  },
  "prettier": {
    "printWidth": 8000,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
