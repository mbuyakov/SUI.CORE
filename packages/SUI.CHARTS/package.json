  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.9",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "husky": "^3.0.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.5.16",
    "@smsoft/sui-core": "^5.4.0"
