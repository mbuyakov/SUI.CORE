{
  "name": "sui",
  "version": "0.0.0",
  "license": "Apache 2.0",
  "scripts": {
    "new-lib": "nx g @sui/nx-plugin:lib",
    "new-react-lib": "test $NAME && nx g @nx/react:library $NAME --unitTestRunner=jest --bundler=rollup --style=@emotion/styled --publishable --importPath @sui/$NAME || echo 'set NAME env'",
    "build": "nx run-many --target=build --parallel=8",
    "build-detailed": "nx run-many --target=build --output-style=stream --parallel=8",
    "test": "nx run-many --target=test --parallel=8",
    "test-detailed": "nx run-many --target=test --output-style=stream --parallel=8",
    "lint": "nx run-many --target=lint --parallel=8",
    "lint-detailed": "nx run-many --target=lint --output-style=stream --parallel=8",
    "ci": "nx run-many --target=lint,test,build",
    "ci-detailed": "nx run-many --target=lint,test,build --output-style=stream --parallel=8",
    "storybook": "nx storybook host-storybook",
    "update-nx": "nx migrate latest",
    "dedupe-lock": "npx yarn-deduplicate yarn.lock --strategy fewer",
    "find-duplicate": "npx yarn-duplicate",
    "clean-cache": "rimraf ./node_modules/.cache",
    "publish": "nx run-many --target=publish"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@nx/devkit": "16.4.0",
    "@swc/helpers": "~0.5.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
