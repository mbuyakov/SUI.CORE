{
  "name": "sui",
  "version": "0.0.0",
  "license": "Apache 2.0",
  "scripts": {
    "postinstall": "patch-package",
    "new-lib": "nx g @sui/nx-plugin:lib",
    "new-react-lib": "nx g @sui/nx-plugin:react-lib",
    "sui-migrator": "nx g @sui/nx-plugin:sui-migrator",
    "index-generator": "nx g @sui/nx-plugin:index",
    "build": "nx run-many --target=build --parallel=8",
    "build-detailed": "nx run-many --target=build --output-style=stream --parallel=8",
    "test": "nx run-many --target=test --parallel=8 --collectCoverage",
    "test-detailed": "nx run-many --target=test --output-style=stream --parallel=8 --collectCoverage",
    "lint": "nx run-many --target=lint --parallel=8",
    "lint-detailed": "nx run-many --target=lint --output-style=stream --parallel=8",
    "ci": "nx run-many --target=lint,test,build --parallel=8 --collectCoverage",
    "ci-detailed": "nx run-many --target=lint,test,build --output-style=stream --parallel=8 --collectCoverage",
    "sonar": "nx run-many --target=test,sonar --output-style=stream --collectCoverage",
    "storybook": "nx storybook host-storybook",
    "update-nx": "nx migrate latest",
    "dedupe-lock": "npx yarn-deduplicate yarn.lock --strategy fewer",
    "find-duplicate": "npx yarn-duplicate",
    "clean-cache": "rimraf ./node_modules/.cache",
    "publish": "nx run-many --target=publish",
    "graph": "nx graph"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@swc/helpers": "~0.5.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@emotion/babel-plugin": "11.11.0",
    "@koliveira15/nx-sonarqube": "3.0.2",
    "@nx/eslint-plugin": "16.6.0",
    "@nx/jest": "16.6.0",
    "@nx/js": "16.6.0",
    "@nx/linter": "16.6.0",
    "@nx/plugin": "16.6.0",
    "@nx/react": "16.6.0",
    "@nx/rollup": "16.6.0",
    "@nx/storybook": "16.6.0",
    "@nx/webpack": "16.6.0",
    "@rollup/plugin-url": "^7.0.0",
    "@storybook/addon-essentials": "7.1.1",
    "@storybook/core-server": "7.1.1",
    "@storybook/react": "7.1.1",
    "@storybook/react-webpack5": "7.1.1",
    "@svgr/rollup": "^8.0.1",
    "@swc-node/register": "^1.6.6",
    "@swc/core": "^1.3.72",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.4.4",
    "@types/node": "18.14.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-jest": "^29.4.1",
    "babel-loader": "^9.1.2",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "29.4.3",
    "jsdom": "~20.0.3",
    "nx": "16.6.0",
    "patch-package": "^8.0.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "storybook": "7.0.23",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "workspaces": [
    "packages/*"
  ]
}
