{
  "extends": ["plugin:@nx/react", "../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ],
  "rules": {
    "@typescript-eslint/no-inferrable-types": 0,
    "no-useless-escape": 0,
    "eqeqeq": 0,
    "no-case-declarations": 0,
    "@typescript-eslint/no-explicit-any": 0,
    "no-extra-boolean-cast": 0,
    "prefer-const": 0,
    "no-control-regex": 0,
    "@typescript-eslint/ban-ts-comment": 0
  }
}
