{
  "name": "@sui/sentry",
  "version": "8.0.0",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "es"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "cross-env ROOT_DIR=src sui-linter",
    "lint-fix": "cross-env ROOT_DIR=src FIX=1 sui-linter",
    "lint-watch": "cross-env ROOT_DIR=src sui-linter --watch",
    "lint-timing": "cross-env TIMING=1 ROOT_DIR=src sui-linter",
    "build": "cd ../.. && cross-env PACKAGE=sentry father-build && cross-env PACKAGE=sentry node ./fix-babel-warning.js && cd ./packages/sentry && npx tsc-alias -p tsconfig.json",
    "ci": "run-s lint build"
  },
  "devDependencies": {
    "@sui/linter": "8.0.0"
  },
  "dependencies": {
    "@sentry/browser": "^5.27.1",
    "@sentry/integrations": "^5.27.1"
  }
}
