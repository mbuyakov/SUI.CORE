{
  "name": "@sui/charts",
  "version": "8.0.0",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "es",
    "styles"
  ],
  "sideEffects": [
    "*.less"
  ],
  "scripts": {
    "lint": "cross-env ROOT_DIR=src sui-linter",
    "lint-fix": "cross-env ROOT_DIR=src FIX=1 sui-linter",
    "lint-watch": "cross-env ROOT_DIR=src sui-linter --watch",
    "lint-timing": "cross-env TIMING=1 ROOT_DIR=src sui-linter",
    "build": "cd ../.. && cross-env PACKAGE=charts father-build && cd ./packages/charts && npx tsc-alias -p tsconfig.json",
    "ci": "run-s lint build"
  },
  "devDependencies": {
    "@sui/linter": "8.0.0"
  },
  "dependencies": {
    "@amcharts/amcharts4": "4.10.15",
    "@sui/core": "8.0.0"
  }
}
